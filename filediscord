const express = require("express");
const app = express();

app.get("/", (req, res) => {
  res.send("Server running");
});

app.get("/callback", (req, res) => {
  res.send("Discord login worked!");
});

const PORT = process.env.PORT || 3000;
app.listen(PORT, () => {
  console.log("Running on port", PORT);
});
{
  "name": "discord-oauth",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
